                  <JFXButton fx:id="btnSalir" contentDisplay="RIGHT" onAction="#click" prefHeight="44.0" prefWidth="152.0" text="Regresar" textFill="WHITE">
                     <graphic>
                        <ImageView fitHeight="39.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../resources/Back.png" />
                           </image>
                        </ImageView>
                     </graphic>
                     <VBox.margin>
                        <Insets bottom="5.0" />
                     </VBox.margin>
