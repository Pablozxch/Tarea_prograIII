            <VBox alignment="CENTER" prefHeight="400.0" prefWidth="86.0" HBox.hgrow="ALWAYS">
               <children>
                  <JFXButton fx:id="btnMarcas" onAction="#onActionBtnMarcas" prefHeight="44.0" prefWidth="152.0" text="Reporte Marcas" textFill="WHITE" />
               </children>
            </VBox>
            <VBox alignment="CENTER" fillWidth="false" prefHeight="200.0" prefWidth="100.0" spacing="45.0" HBox.hgrow="ALWAYS">
               <children>
                  <JFXTextField fx:id="txtFolio" promptText="Folio Empleado">
